{
    "name": "dreamcommerce/object-audit-bundle",
    "license": "LGPL-2.1",
    "keywords": [
        "Persistence",
        "Database",
        "Audit"
    ],
    "description": "Audit for Doctrine entities & Sylius resources",
    "require": {
        "php": "~7.0",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.4",
        "ocramius/proxy-manager": "^2.0",
        "dreamcommerce/common-bundle": "dev-master",
        "dreamcommerce/bugtracker": "^1.1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.4",
        "gedmo/doctrine-extensions": "^2.3.1",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "phpunit/phpunit": "^5.5",
        "symfony/framework-bundle": "^3.2",
        "symfony/var-dumper": "^3.2",
        "sylius/resource-bundle": "v1.0.0-beta.1@dev",
        "friendsofphp/php-cs-fixer": "^1.11",
        "polishsymfonycommunity/symfony-mocker-container": "^1.0",
        "symfony/browser-kit": "^3.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "gedmo/doctrine-extensions": "<2.3.1",
        "symfony/framework-bundle": "<3.2",
        "doctrine/doctrine-bundle": "<1.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "test": [
            "@test-php-7.0",
            "@test-php-7.1"
        ],
        "cs-check": "bin/php-cs-fixer --version && bin/php-cs-fixer fix -v --diff --dry-run src",
        "cs-fix": "bin/php-cs-fixer fix -v src",
        "test-php-7.0": "bin/docker-phpunit-7.0",
        "test-php-7.1": "bin/docker-phpunit-7.1",
        "post-install-cmd": [
            "DreamCommerce\\Component\\Common\\Composer\\ScriptHandler::copyTemplateDir"
        ],
        "post-update-cmd": [
            "DreamCommerce\\Component\\Common\\Composer\\ScriptHandler::copyTemplateDir"
        ]
    },
    "autoload": {
        "psr-4": {
            "DreamCommerce\\Component\\ObjectAudit\\": "src/DreamCommerce/Component/ObjectAudit",
            "DreamCommerce\\Bundle\\ObjectAuditBundle\\": "src/DreamCommerce/Bundle/ObjectAuditBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DreamCommerce\\Tests\\ObjectAudit\\": "test/src/DreamCommerce/Tests/ObjectAudit",
            "DreamCommerce\\Tests\\ObjectAuditBundle\\": "test/src/DreamCommerce/Tests/ObjectAuditBundle",
            "AppBundle\\": "test/src/AppBundle/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dreamcommerce/common-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dreamcommerce/bugtracker-bundle"
        }
    ]
}
