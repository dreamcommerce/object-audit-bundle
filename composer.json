{
    "name": "dreamcommerce/object-audit-bundle",
    "license": "LGPL-2.1",
    "keywords": [
        "Persistence",
        "Database",
        "Audit"
    ],
    "description": "Audit for Doctrine entities & Sylius resources",
    "require": {
        "php": "~7.0",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.4",
        "ocramius/proxy-manager": "^2.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.4",
        "gedmo/doctrine-extensions": "^2.3.1",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "phpunit/phpunit": "^5.5",
        "symfony/framework-bundle": "^3.2",
        "symfony/var-dumper": "^3.2",
        "sylius/resource-bundle": "v1.0.0-beta.1@dev",
        "friendsofphp/php-cs-fixer": "^1.11"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "gedmo/doctrine-extensions": "<2.3.1",
        "symfony/framework-bundle": "<3.2",
        "doctrine/doctrine-bundle": "<1.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "bin/php-cs-fixer --version && bin/php-cs-fixer fix -v --diff --dry-run src",
        "cs-fix": "bin/php-cs-fixer fix -v src",
        "test": "bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "DreamCommerce\\Component\\ObjectAudit\\": "src/DreamCommerce/Component/ObjectAudit",
            "DreamCommerce\\Bundle\\ObjectAuditBundle\\": "src/DreamCommerce/Bundle/ObjectAuditBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DreamCommerce\\Tests\\ObjectAuditBundle\\": "tests/DreamCommerce/Tests/ObjectAuditBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    }
}
